{
    "name": "Unblur The Page",
    "description": "Unblur the page duh!",
    "version": "1.9.1",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["jquery.js", "background.js", "onInstall.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Unblur this doc"
    },
    "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
    "manifest_version": 2
}
