{

  "manifest_version": 2,
  "name": "Unblur The Page",
  "version": "1.0",
  "description": "Unblurs the page duh!",

  "background": {
    "scripts": ["background.js"]
  },

  "icons": {
    "48": "icon.png"
  },

  "browser_action": {
    "default_icon": {
      "48": "icon.png"
    }
  },  

  "permissions": [
    "tabs",
    "activeTab"
  ]
  
}
